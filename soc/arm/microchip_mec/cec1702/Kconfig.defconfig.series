if SOC_SERIES_CEC1702

config SOC_SERIES
        default "cec1702"

config NUM_IRQS
	int
	# must be >= the highest interrupt number used
	# - include the UART interrupts
	default 42

config CORTEX_M_SYSTICK
	default y

source "soc/arm/microchip_mec/cec1702/Kconfig.defconfig.cec1702*"

endif
