/dts-v1/;

#include "microchip/cec1702.dtsi"

/ {
	model = "Microchip SecureIoT1702 board";
	compatible = "microchip,secureiot1702", "microchip,cec1702";

	chosen {
		zephyr,sram = &sram0;
		zephyr,console = &uart1;
		zephyr,flash = &flash0;
	};
};

&uart0 {
	status = "okay";
	current-speed = <115200>;
};

&uart1 {
	status = "okay";
	current-speed = <115200>;
};

&spi0 {
	status = "okay";
};

&gpspi0 {
	status = "okay";
};
